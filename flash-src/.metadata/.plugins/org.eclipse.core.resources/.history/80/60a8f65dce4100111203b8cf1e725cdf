package com.dozeo.pusheras.auth
{
	import flash.net.URLLoader;
	import flash.net.URLRequest;
	import flash.net.URLVariables;

	public class PusherAuthenticator
	{
		public function PusherAuthenticator()
		{
		}
		
		public static function authenticate(channelName:String, socketID:Number, endPoint:String):String
		{
			var urlLoader:URLLoader = new URLLoader();
			var urlRequest:URLRequest = new URLRequest(endPoint);
			var postVars:URLVariables = new URLVariables();
		}
	}
}